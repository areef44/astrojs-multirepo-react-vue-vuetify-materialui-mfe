{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "esnext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": ["DOM", "ESNext"],
    "types": ["vite/client", "vue", "react", "react-dom"]
  },
  "include": [
    "src/**/*",
    "astro.config.ts",
    "vite.config.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}